{
  "name": "dynamic-page",
  "version": "1.14.5",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "npm run prebuild && father-build && mv dist/es es && mv dist/lib lib && mv dist/umd umd && rm -rf dist && cp src/components/icon/iconfont.js lib && cp src/components/icon/iconfont.js umd && cp src/components/icon/iconfont.js es",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prebuild": "rm -rf lib && rm -rf es && rm -rf umd"
  },
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "license": "GPL",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "querystring": "^0.2.1",
    "react": "^16.12.0 || ^17.0.0",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.176",
    "@types/uuid": "^8.3.1",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.0.17",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
